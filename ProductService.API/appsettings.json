{
  "ConnectionStrings": {
    "ProductServiceConnection": "THIS IS SECRET"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": { "name": "ProductService" },
    "cloud": {
      "config": {
        "uri": "http://localhost:5006",
        "validate_certificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "ProductService",
      "port": "5006",
      "hostName": "localhost",
      "healthCheckUrlPath": "/health",
      "statusPageUrlPath": "/health",
      "instanceId": "localhost:5006"
    }
  }
}
